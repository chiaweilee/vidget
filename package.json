{
  "name": "vue-widget",
  "version": "0.0.0",
  "scripts": {
    "lint": "eslint --ext .js,.vue,.json src --fix",
    "es5": "npx babel --no-babelrc src --out-dir lib",
    "test": "mocha",
    "build": "npx webpack --config webpack.config.js",
    "commit": "npx git-cz",
    "flow": "flow"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verify-commit-msg.js"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "author": "+v",
  "license": "MIT",
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-flow": "^6.23.0",
    "chai": "^4.1.2",
    "eslint": "^5.9.0",
    "eslint-plugin-vue": "^4.7.1",
    "flow-bin": "^0.86.0",
    "flow-remove-types": "^1.2.3",
    "lint-staged": "^7.3.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.0",
    "remove-flow-types-loader": "^1.1.0",
    "sass-loader": "^7.0.1",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2"
  }
}
